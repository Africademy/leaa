{
  "name": "@leaa/dashboard",
  "author": "solidzoro@live.com",
  "version": "0.0.2",
  "private": false,
  "sideEffect": false,
  "license": "BSD-3-Clause-Clear",
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "---------------------------------------------------------------------- LINT ----": "----",
    "lint-js": "eslint \"src/**/*.{ts,tsx}\"",
    "fix-js": "yarn run lint-js --fix",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "fix-css": "yarn run lint-css --fix",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix": "yarn run fix-js && yarn run fix-css",
    "tsc-check": "tsc --noEmit",
    "---------------------------------------------------------------------- DEV ----": "----",
    "dev-server": "NODE_ENV=development cross-env webpack-dev-server -r dotenv/config --progress --config ./tools/webpack/webpack.config.js --hot --inline --hide-modules --server --host 0.0.0.0",
    "dev": "NODE_ENV=development yarn dev-server",
    "clear": "cross-env rimraf _dist",
    "---------------------------------------------------------------------- PROD ----": "----",
    "build": "yarn clear && NODE_ENV=production cross-env webpack --progress --config ./tools/webpack/webpack.config.js --release",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "u": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@toast-ui/react-editor": "^1.0.1",
    "animated-scroll-to": "^2.0.3",
    "antd": "^3.24.2",
    "codemirror": "^5.49.2",
    "core-js": "3.3.4",
    "dnd-core": "^9.4.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "history": "^4.10.1",
    "i18next": "^18.0.0",
    "i18next-browser-languagedetector": "^4.0.0",
    "i18next-xhr-backend": "^3.2.0",
    "immutability-helper": "^3.0.1",
    "mobx": "^5.14.2",
    "mobx-react": "6.1.4",
    "normalize.css": "^8.0.1",
    "query-string": "6.8.3",
    "react-apollo": "^3.1.3",
    "react-css-modules": "4.7.11",
    "react-dnd": "^9.4.0",
    "react-dnd-html5-backend": "^9.4.0",
    "react-dropzone": "^10.1.10",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.0.0",
    "react-router-dom": "5.1.2",
    "tui-editor": "^1.4.7",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/draft-js": "^0.10.37",
    "@types/history": "^4.7.3",
    "@types/lodash": "4.14.144",
    "@types/react-dom": "^16.9.3",
    "@types/react-helmet": "^5.0.13",
    "@types/react-router-dom": "^5.1.0",
    "@types/uuid": "^3.4.5",
    "@types/webpack-env": "1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
