  "license": "BSD-3-Clause-Clear",
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "---------------------------------------------------------------------- LINT ----": "----",
    "lint-js": "eslint \"src/**/*.{ts,tsx}\"",
    "fix-js": "yarn run lint-js --fix",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "fix-css": "yarn run lint-css --fix",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix": "yarn run fix-js && yarn run fix-css",
    "---------------------------------------------------------------------- DEV ----": "----",
    "dev-server": "NODE_ENV=development cross-env webpack-dev-server -r dotenv/config --progress --config ./tools/webpack/webpack.config.js --hot --inline --hide-modules --server --host 0.0.0.0",
    "dev": "NODE_ENV=development yarn dev-server",
    "tsc-check": "tsc --noEmit",
    "clear": "cross-env rimraf _dist",
    "---------------------------------------------------------------------- PROD ----": "----",
    "build": "yarn clear && NODE_ENV=production cross-env webpack --progress --config ./tools/webpack/webpack.config.js --release",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "u": "yarn upgrade-interactive --latest"
  },
