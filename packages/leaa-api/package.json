{
  "name": "@leaa/api",
  "author": "solidzoro@live.com",
  "version": "0.0.3",
  "private": false,
  "sideEffect": false,
  "license": "BSD-3-Clause-Clear",
  "engines": {
    "node": "10.x.x",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "---------------------------------------------------------------------- LINT ----": "----",
    "lint-js": "eslint \"src/**/*.{ts,tsx}\"",
    "lint": "yarn run lint-js",
    "fix-js": "yarn run lint-js --fix",
    "fix": "yarn run fix-js",
    "---------------------------------------------------------------------- TEST ----": "----",
    "test": "jest --verbose --config=jest.config.js",
    "test:codecov": "jest --coverage --detectOpenHandles --forceExit --config=jest.config.js",
    "test:watch": "jest --watch --verbose --config=jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e": "jest --config=jest-e2e.config.js --forceExit",
    "codecov": "codecov",
    "tsc-check": "tsc --noEmit",
    "---------------------------------------------------------------------- DEV ----": "----",
    "dev-tsnodedev": "NODE_ENV=development ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register ./src/main.ts",
    "dev-nodemon": "NODE_ENV=development nodemon",
    "dev": "NODE_ENV=development yarn dev-tsnodedev",
    "seed": "NODE_ENV=development ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register ./src/seed.ts",
    "nodemon": "NODE_ENV=development nodemon",
    "clear": "rimraf _dist",
    "---------------------------------------------------------------------- PROD ----": "----",
    "prod": "NODE_ENV=production ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register ./src/main.ts",
    "---------------------------------------------------------------------- SERVER ----": "----",
    "build": "yarn clear && NODE_ENV=production tsc -b tsconfig.build.json",
    "start": "NODE_ENV=production node index.js",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "u": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@nestjs/common": "^6.8.4",
    "@nestjs/core": "^6.8.4",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.4",
    "@nestjs/testing": "^6.8.4",
    "@nestjs/typeorm": "^6.2.0",
    "ali-oss": "^6.1.1",
    "apollo-server-core": "^2.9.7",
    "apollo-server-express": "^2.9.7",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "core-js": "3.3.4",
    "dotenv": "^8.2.0",
    "envalid": "^5.0.0",
    "express": "^4.17.1",
    "graphql-middleware": "^4.0.1",
    "graphql-shield": "^7.0.0",
    "graphql-tools": "^4.0.6",
    "html-to-text": "^5.1.1",
    "image-size": "^0.8.3",
    "jimp": "^0.8.5",
    "jsondiffpatch": "^0.3.11",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "nodejieba": "^2.3.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path-to-regexp": "^3.1.0",
    "query-string": "^6.8.3",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.3",
    "wechat-jssdk": "^5.0.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.19",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.144",
    "@types/mkdirp": "^0.5.2",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.11.7",
    "@types/passport-jwt": "^3.0.2",
    "@types/request": "^2.48.3",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint-config-airbnb-typescript": "^6.0.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "nodemon": "^1.19.4"
  }
}
