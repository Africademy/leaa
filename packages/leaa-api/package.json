{
  "name": "@leaa/api",
  "author": "solidzoro@live.com",
  "version": "0.0.1",
  "private": true,
  "sideEffect": false,
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "---------------------------------------------------------------------- LINT ----": "----",
    "lint-js": "eslint \"src/**/*.{ts,tsx}\"",
    "lint": "yarn run lint-js",
    "fix-js": "yarn run lint-js --fix",
    "fix": "yarn run fix-js",
    "---------------------------------------------------------------------- TEST ----": "----",
    "test": "NODE_ENV=development jest --watch --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "---------------------------------------------------------------------- DEV ----": "----",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register ./src/main.ts",
    "seed": "NODE_ENV=development ts-node -r tsconfig-paths/register ./src/seed.ts",
    "nodemon": "NODE_ENV=development nodemon",
    "tsc": "tsc --noEmit",
    "---------------------------------------------------------------------- PROD ----": "----",
    "server": "pm2-docker start ./pm2.json",
    "prod": "NODE_ENV=production ts-node-dev --respawn --transpileOnly ./src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "u": "yarn upgrade-interactive --latest",
    "s": "yarn dev"
  },
  "dependencies": {
    "@leaa/common": "0.0.1",
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/graphql": "^6.2.4",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/typeorm": "^6.1.2",
    "apollo-server-core": "^2.6.3",
    "apollo-server-express": "^2.6.3",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^5.3.8",
    "graphql-tools": "^4.0.4",
    "joi": "^14.3.1",
    "jsondiffpatch": "^0.3.11",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "type-graphql": "^0.17.4",
    "typeorm": "^0.2.18",
    "typescript": "3.5.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.15",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.8",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "concurrently": "^4.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.18.1",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tsconfig-paths": "3.8.0",
    "typescript-eslint-parser": "^22.0.0",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
