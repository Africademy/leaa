{
  "name": "@leaa/api",
  "author": "solidzoro@live.com",
  "version": "0.0.1",
  "private": true,
  "sideEffect": false,
  "license": "GPL",
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "---------------------------------------------------------------------- LINT ----": "----",
    "lint-js": "eslint \"src/**/*.{ts,tsx}\"",
    "lint": "yarn run lint-js",
    "fix-js": "yarn run lint-js --fix",
    "fix": "yarn run fix-js",
    "---------------------------------------------------------------------- TEST ----": "----",
    "test": "jest --verbose --config=jest.config.js",
    "test:codecov": "jest --verbose --coverage --config=jest.config.js",
    "test:watch": "jest --watch --verbose --config=jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e": "jest --config=jest-e2e.config.js --forceExit",
    "codecov": "codecov",
    "tsc-check": "tsc --noEmit",
    "---------------------------------------------------------------------- DEV ----": "----",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register ./src/main.ts",
    "seed": "NODE_ENV=development ts-node -r tsconfig-paths/register ./src/seed.ts",
    "nodemon": "NODE_ENV=development nodemon",
    "clear": "rimraf _dist",
    "---------------------------------------------------------------------- PROD ----": "----",
    "prod": "NODE_ENV=production ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register ./src/main.ts",
    "---------------------------------------------------------------------- SERVER ----": "----",
    "build": "yarn clear && NODE_ENV=production tsc -b tsconfig.build.json",
    "start": "NODE_ENV=production node server.js",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "u": "yarn upgrade-interactive --latest",
    "s": "yarn dev"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/graphql": "^6.4.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.5.3",
    "@nestjs/typeorm": "^6.1.3",
    "apollo-server-core": "^2.8.1",
    "apollo-server-express": "^2.8.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "envalid": "^5.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-middleware": "^3.0.3",
    "graphql-shield": "^6.0.3",
    "graphql-tools": "^4.0.5",
    "image-size": "^0.7.4",
    "jimp": "^0.6.4",
    "jsondiffpatch": "^0.3.11",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "module-alias": "^2.2.1",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "type-graphql": "^0.17.4",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "@leaa/common": "^0.0.1",
    "@nestjs/testing": "^6.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/hapi__joi": "^15.0.3",
    "@types/image-size": "^0.7.0",
    "@types/jest": "^24.0.16",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.136",
    "@types/mkdirp": "^0.5.2",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.6.9",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "codecov": "^3.5.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.18.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tsconfig-paths": "^3.8.0",
    "typescript": "3.5.3",
    "typescript-eslint-parser": "^22.0.0"
  }
}
