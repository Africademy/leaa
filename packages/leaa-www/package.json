    "module-alias": "^2.2.2",
    "next": "^9.1.4",
    "node-fetch": "^2.6.0",
    "nookies": "^2.0.8",
    "nprogress": "^0.2.0",
    "null-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react-css-modules": "4.7.11",
    "react-headroom": "^3.0.0",
