{
  "name": "@leaa/www",
  "author": "solidzoro@live.com",
  "version": "0.0.2",
  "private": false,
  "sideEffect": false,
  "license": "BSD-3-Clause-Clear",
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "---------------------------------------------------------------------- LINT ----": "----",
    "lint-js": "eslint \"**/*.{ts,tsx}\"",
    "fix-js": "yarn run lint-js --fix",
    "lint-css": "stylelint \"**/*.{css,less,styl,scss,sass,sss}\"",
    "fix-css": "yarn run lint-css --fix",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix": "yarn run fix-js && yarn run fix-css",
    "tsc-check": "tsc --noEmit",
    "---------------------------------------------------------------------- DEV ----": "----",
    "dev": "NODE_ENV=development nodemon server.ts",
    "clear": "rimraf .next && rimraf _dist",
    "---------------------------------------------------------------------- PROD ----": "----",
    "build": "yarn clear && NODE_ENV=production next build && tsc --project tsconfig.build.json",
    "start-by-next": "NODE_ENV=production next start",
    "start": "NODE_ENV=production node ./index.js",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "u": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "@zeit/next-less": "1.0.1",
    "class-validator": "^0.10.2",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "3.2.0",
    "dayjs": "^1.8.16",
    "express": "^4.17.1",
    "file-loader": "^4.1.0",
    "history": "^4.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lodash-webpack-plugin": "^0.11.5",
    "module-alias": "^2.2.2",
    "next": "^9.1.1",
    "node-fetch": "^2.6.0",
    "nookies": "^2.0.8",
    "nprogress": "^0.2.0",
    "null-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react-css-modules": "4.7.11",
    "react-headroom": "^2.2.8",
    "react-id-swiper": "^2.3.2",
    "rimraf": "^3.0.0",
    "swiper": "^5.0.4",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.20",
    "ua-parser-js": "^0.7.20",
    "url-loader": "^2.2.0",
    "vconsole": "^3.3.4",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.18",
    "@types/js-cookie": "^2.2.3",
    "@types/lodash": "4.14.144",
    "@types/nprogress": "^0.2.0",
    "@types/react-dom": "^16.9.1",
    "@types/react-headroom": "^2.2.1",
    "@types/ua-parser-js": "^0.7.33"
  }
}
