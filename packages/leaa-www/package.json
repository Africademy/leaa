{
  "name": "@leaa/www",
  "author": "solidzoro@live.com",
  "version": "0.0.4",
  "private": false,
  "sideEffect": false,
  "license": "BSD-3-Clause-Clear",
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.0"
  },
  "scripts": {
    "---------------------------------------------------------------------- LINT ----": "----",
    "lint-js": "eslint \"**/*.{ts,tsx}\"",
    "fix-js": "yarn run lint-js --fix",
    "lint-css": "stylelint \"**/*.{css,less,styl,scss,sass,sss}\"",
    "fix-css": "yarn run lint-css --fix",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix": "yarn run fix-js && yarn run fix-css",
    "tsc-check": "tsc --noEmit",
    "---------------------------------------------------------------------- DEV ----": "----",
    "dev": "NODE_ENV=development nodemon server.ts",
    "clear": "rimraf .next && rimraf _dist",
    "---------------------------------------------------------------------- PROD ----": "----",
    "build": "yarn clear && NODE_ENV=production next build && tsc --project tsconfig.build.json",
    "start-by-next": "NODE_ENV=production next start",
    "export": "next build && NODE_ENV=production next export -o _out",
    "start": "NODE_ENV=production node ./index.js",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "u": "yarn upgrade-interactive --latest"
  },
